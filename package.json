{
  "name": "juicebox-os",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "wasm:build": "emcc src/os/main.cpp -o src/wasm/juicebox.mjs -sWASM=1 -sMODULARIZE=1 -sEXPORT_NAME=createJuiceboxModule -sEXPORTED_FUNCTIONS=['_handle_command','_hello', '_add','_subtract','_multiply', '_divide'] -sEXPORTED_RUNTIME_METHODS=cwrap,ccall -O2",
    "dev": "npm run wasm:build && vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },

  "dependencies": {
    "@tailwindcss/vite": "^4.1.10",
    "framer-motion": "^12.18.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-draggable": "^4.4.6",
    "tailwindcss": "^4.1.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
